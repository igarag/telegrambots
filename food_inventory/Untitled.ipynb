   "source": [
    "import json\n",
    "\n",
    "raw = {\n",
    "  \"tomate\": 2,\n",
    "  \"colines\": 300,\n",
    "  \"cerveza\": 0\n",
    "}\n",
    "data = json.dumps(raw)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "inventory = json.loads(data)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "response = \"\"\n",
    "for element, value in inventory.items():\n",
    "    response += f\"- {element}: {value} \\n\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "- tomate: 2 \n",
      "- colines: 300 \n",
      "- cerveza: 0 \n",
      "\n"
     ]
    }
   ],
   "source": [
    "print(response)"
   ]
  },
